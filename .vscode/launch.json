{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/install/vslam_ros/lib/${input:package}/${input:program}",
        "args": ["--ros-args","--params-file","${workspaceFolder}/config/nodeMapping.yaml"],
        //"preLaunchTask": "build",
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "additionalSOLibSearchPath": "${workspaceFolder/install/vslam_ros/lib"
      },
    {
      "name": "(gdb) Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/vslam_ros/src/vslam/test/${input:test}",
      "args": [],
      "preLaunchTask": "build-debug",
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) Run Executable",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/bin/${input:target}",
      "args": ["--ros-args","--params-file","${workspaceFolder}/RgbdAlignmentNode.yaml"],
      "preLaunchTask": "build",
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "additionalSOLibSearchPath": "${workspaceFolder/install/lib",
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) Run Rgbd Alignment",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/bin/rgbdAlignment",
      "args": ["--ros-args","--params-file","${workspaceFolder}/src/vslam_ros/vslam_ros/config/RgbdAlignmentNodeParams.yaml"],
      "preLaunchTask": "build",
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "additionalSOLibSearchPath": "${workspaceFolder/install/lib",
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) Launch Test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/vslam_ros/src/vslam/${input:test}",
      "args": [],
      "preLaunchTask": "build_vslam",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
        "name": "(gdb) Launch App",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/vslam_ros/src/vslam/evaluate_tum",
        "args": [],
        "preLaunchTask": "build_app",
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      },
      {
        "name": "(gdb) Launch App DVO",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/vslam_ros/src/vslam/src/app/compare_dvo",
        "preLaunchTask": "build_app_dvo",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      },
    // Example launch of a python file
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
    },
    {
        "name": "Evaluate RPE",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/script/vslam_evaluation/tum/evaluate_rpe.py",
        "args": ["/mnt/dataset/tum_rgbd/${input:sequence_id}/${input:sequence_id}-groundtruth.txt", 
                "/mnt/dataset/tum_rgbd/${input:sequence_id}/algorithm_results/${input:experiment_name}/${input:sequence_id}-algo.txt",
                "--fixed_delta","--verbose"],
        "console": "integratedTerminal",
      },
    // Example gdb launch of a ros executable
    {
      "name": "(gdb) Launch ROS node",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/${input:package}/lib/${input:package}/${input:program}",
      "args": ["--ros-args","--params-file","${workspaceFolder}/config/nodeMapping.yaml"],
      "preLaunchTask": "build",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "additionalSOLibSearchPath": "${workspaceFolder/install/vslam_ros/lib",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
        "name": "(gdb) Launch Composition",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/install/vslam_ros/lib/composition_evaluation_tum",
        "args": ["--ros-args","--params-file","${workspaceFolder}/config/node_config.yaml",
        "-p","bag_file:=/mnt/dataset/tum_rgbd/${input:sequence_id}",
        "-p","gtTrajectoryFile:=/mnt/dataset/tum_rgbd/${input:sequence_id}/${input:sequence_id}-groundtruth.txt",
        "-p","algoOutputFile:=/mnt/dataset/tum_rgbd/${input:sequence_id}/algorithm_results/${input:experiment_name}/${input:sequence_id}-algo.txt",
        "-p","replayMode:=True"],
        //"preLaunchTask": "build",
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": false,
        "MIMode": "gdb",
        "additionalSOLibSearchPath": "${workspaceFolder/install/vslam_ros/lib",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
  ],
  "inputs": [
    {
      "id": "package",
      "type": "promptString",
      "description": "Package name",
      "default": "vslam_ros"
    },
    {
      "id": "program",
      "type": "promptString",
      "description": "Program name",
      "default": "nodeMapping"
    },    
    {
        "id": "target",
        "type": "pickString",
        "description": "Run target",
        "options":[
          "evaluation_app",
          "rgbdAlignmentComp",
          "rgbdAlignment"

        ]
      },
      {
        "id": "module",
        "type": "pickString",
        "description": "vslam module",
        "default": "odometry",
        "options":[
          "odometry",
          "core"
        ]
      },
      {
        "id": "test",
        "type": "pickString",
        "description": "vslam unit test",
        "options":[
            "frame2Test",
            "rgbd_alignmentTest",
            "rpeTest",
            "motion_modelTest",
            "ekf_se3Test",
            "trackingTest",
          "trajectoryTest",
          "mapTest",
          "frameTest",
        ]
      },
      {
        "id": "sequence_id",
        "type": "pickString",
        "description": "sequence",
        "options": [
            "rgbd_dataset_freiburg1_desk",
            "rgbd_dataset_freiburg1_desk_validation",
            "rgbd_dataset_freiburg1_desk2",
            "rgbd_dataset_freiburg1_desk2_validation",
            "rgbd_dataset_freiburg1_floor",
            "rgbd_dataset_freiburg1_rpy",
            "rgbd_dataset_freiburg1_xyz",
            "rgbd_dataset_freiburg1_360",
            "rgbd_dataset_freiburg2_desk",
            "rgbd_dataset_freiburg2_desk_validation",
            "rgbd_dataset_freiburg2_pioneer_360",
            "rgbd_dataset_freiburg2_pioneer_slam",
            "rgbd_dataset_freiburg3_long_office_household",
        ]
    },
    {
        "id": "experiment_name",
        "type": "promptString",
        "description": "name of experiment",
        "default": "test"
    },
  ]
}